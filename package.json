{
  "name": "bibmeta",
  "version": "1.0.0",
  "description": "bibmeta nft",
  "files": [
    "/contracts/**/*.sol"
  ],
  "scripts": {
    "run-env": "npm i && tail -f /dev/null",
    "hardhat": "hardhat",
    "hardhat-bsc-test": "cross-env hardhat --network bsc_test",
    "hardhat-bsc": "cross-env hardhat --network bsc",
    "hardhat help": "hardhat help",
    "compile": "SKIP_LOAD=true hardhat compile",
    "compile:windows": "hardhat compile",
    "compile:force": "npm run compile -- --force",
    "compile:force:quiet": "npm run compile:force -- --quiet",
    "types-gen": "hardhat typechain",
    "test": "TS_NODE_TRANSPILE_ONLY=1 hardhat test",
    "coverage": "SKIP_LOAD=true npx hardhat typechain && node --max_old_space_size=6144 node_modules/.bin/hardhat coverage",
    "dev:deployment": "hardhat dev-deployment ",
    "bsc-test:upgrade-contract": "npm run hardhat-bsc-test -- upgrade-contract",
    "bsc-test:upgrade-market-impl": "npm run hardhat-bsc-test -- upgrade:marketImpl",
    "bs-test:upgrade-staked": "npm run hardhat-bsc-test -- upgrade:stakedImpl",
    "bs-test:upgrade-staked-dividend": "npm run hardhat-bsc-test -- upgrade:stakedDividendImpl",
    "bsc-test:transfer-ownership": "npm run hardhat-bsc-test -- migrate-owner-and-admin --no-admin-change",
    "bsc:transfer-ownership": "npm run hardhat-bsc -- migrate-owner-and-admin --noAdminChange",
    "bsc-test:deployment": "npm run hardhat-bsc-test -- bsc-test-deployment --verify",
    "bsc:deployment": "npm run hardhat-bsc -- bsc-deployment --verify",
    "dev-deployment": "npm run hardhat -- dev-deployment",
    "bsc-test:run-exclude": "npm run hardhat-bsc-test -- run-exclude",
    "bsc:run-exclude": "npm run hardhat-bsc -- run-exclude",
    "bsc-test:enable-dividend": "npm run hardhat-bsc-test -- enable-dividend --enable",
    "bsc-test:disable-dividend": "npm run hardhat-bsc-test -- enable-dividend",
    "bsc:enable-dividend": "npm run hardhat-bsc-test -- enable-dividend --enable",
    "bsc:disable-dividend": "npm run hardhat-bsc-test -- enable-dividend",
    "ci:clean": "rm -rf ./artifacts ./cache ./types"
  },
  "devDependencies": {
    "@nomiclabs/buidler": "^1.4.7",
    "@nomiclabs/hardhat-ethers": "^2.0.1",
    "@nomiclabs/hardhat-etherscan": "^2.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.1.0",
    "@openzeppelin/hardhat-upgrades": "^1.17.0",
    "@typechain/ethers-v5": "^4.0.0",
    "@types/chai": "4.2.11",
    "@types/lowdb": "1.0.9",
    "@types/mocha": "7.0.2",
    "@types/node": "14.0.5",
    "@uniswap/v2-periphery": "1.1.0-beta.0",
    "bignumber.js": "9.0.0",
    "buidler-typechain": "0.1.1",
    "chai": "4.2.0",
    "dotenv": "^8.2.0",
    "chai-bignumber": "3.0.0",
    "cross-env": "^7.0.3",
    "eth-sig-util": "2.5.3",
    "ethereum-waffle": "^3.2.1",
    "ethereumjs-util": "7.0.2",
    "ethers": "^5.0.8",
    "hardhat": "^2.0.4",
    "hardhat-abi-exporter": "^2.10.0",
    "hardhat-typechain": "^0.3.3",
    "husky": "^4.2.5",
    "lowdb": "1.0.0",
    "prettier": "^2.0.5",
    "prettier-plugin-solidity": "^1.0.0-alpha.53",
    "prompt-sync": "^4.2.0",
    "solidity-coverage": "^0.7.13",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.8.1",
    "typechain": "^3.0.0",
    "typescript": "^4.1.2"
  },
  "author": "BIB",
  "contributors": [],
  "license": "AGPLv3",
  "dependencies": {
    "@openzeppelin/contracts-upgradeable": "^4.7.3",
    "delayed-stream": "^1.0.0",
    "dotenv": "^8.2.0",
    "tmp-promise": "^3.0.2"
  }
}
